<div class="container-fluid">    <div class="row">        <div class="col s12 m6 offset-m3">            <div class="person-card card">                <div class="card-content">                    <h3 class="card-title center-align deep-purple-text">                        <%= first_name %> <%= last_name %>                    </h3>                    <form id="edit_person">                        <div class="row">                            <div class="col s12">                                <label for="_id">Person ID:</label>                                <input type="text" id="_id" name="_id" disabled value="<%= _id %>"                                       placeholder="Person ID" class="validate"/>                            </div>                        </div>                        <div class="row">                            <div class="col s12">                                <label for="first_name">First Name:</label>                                <input type="text" id="first_name" name="first_name" placeholder="First Name"                                       value="<%= first_name %>" class="validate"/>                            </div>                        </div>                        <div class="row">                            <div class="col s12">                                <label for="last_name">Last Name:</label>                                <input type="text" id="last_name" name="last_name" placeholder="Last Name"                                       value="<%= last_name %>" class="validate"/>                            </div>                        </div>                        <div class="row">                            <div class="col s12">                                <label for="date_of_birth">Date Of Birth:</label>                                <input type="text" id="date_of_birth" name="date_of_birth" placeholder="Date of Birth"                                       value="<%= date_of_birth %>" class="validate"/>                            </div>                        </div>                        <div class="row">                            <div class="col s12">                                <label for="zip">Zip Code:</label>                                <input type="text" id="zip" name="zip" placeholder="Zip Code" value="<%= zip %>"                                       class="validate"/>                            </div>                        </div>                    </form>                </div>                <div class="card-action center-align">                    <div class="row">                        <div class="col s12 m4">                            <button id="cancel" class="waves-effect waves-light btn deep-purple btn-app">                                <i class="fa fa-times"></i>                                Cancel                            </button>                        </div>                        <div class="col s12 m4">                            <button id="delete" class="waves-effect waves-light center-align btn light-blue btn-app">                                <i class="fa fa-trash"></i>                                Delete                            </button>                        </div>                        <div class="col s12 m4">                            <button id="save" class="waves-effect waves-light btn deep-purple btn-app">                                <i class="fa fa-check"></i>                                Edit User                            </button>                        </div>                    </div>                </div>            </div>        </div>    </div></div>